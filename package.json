{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "./build.sh",
    "build-google-feed": "ts-node ./src/google-feed.ts",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently yarn:start:*",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "codegen": "graphql-codegen --config codegen.json"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "3.4.2",
    "@vendure/asset-server-plugin": "3.4.2",
    "@vendure/core": "3.4.2",
    "@vendure/email-plugin": "3.4.2",
    "@vendure/graphiql-plugin": "3.4.2",
    "dotenv": "17.2.2",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@angular-devkit/core": "^19.2.17",
    "@angular/cli": "^19.2.17",
    "@vendure/cli": "3.4.2",
    "@vendure/ui-devkit": "3.4.2",
    "concurrently": "9.2.1",
    "typescript": "5.8.2"
  }
}
