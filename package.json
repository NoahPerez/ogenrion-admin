{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev:admin-ui": "ts-node src/custom-admin-ui/compile-admin-ui.ts --dev --watch",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:admin-ui\"",
    "build:admin-ui": "ts-node src/custom-admin-ui/compile-admin-ui.ts",
    "build:admin-ui:dev": "ts-node src/custom-admin-ui/compile-admin-ui.ts --dev",
    "build": "npm run build:admin-ui && tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently yarn:start:*"
  },
  "dependencies": {
    "@vendure/admin-ui-plugin": "3.4.2",
    "@vendure/asset-server-plugin": "3.4.2",
    "@vendure/core": "3.4.2",
    "@vendure/email-plugin": "3.4.2",
    "@vendure/graphiql-plugin": "3.4.2",
    "@vendure/ui-devkit": "3.4.2",
    "concurrently": "9.2.1",
    "dotenv": "17.2.2",
    "pg": "8.16.3"
  },
  "devDependencies": {
    "@vendure/cli": "3.4.2",
    "typescript": "5.8.2"
  }
}
